{
  "scripts": {
    "import": "node src/scripts/runTs.js import ts",
    "chart": "node src/scripts/runTs.js chart ts",
    "start": "node src/scripts/runTs.js start ts",
    "dev": "node src/scripts/runTs.js dev ts",
    "cluster": "node src/scripts/runTs.js cluster ts",
    "start:js": "node dist/src/scripts/runJs.js start js",
    "dev:js": "node dist/src/scripts/runJs.js dev js",
    "cluster:js": "node dist/src/scripts/runJs.js cluster js",
    "format": "prettier --write",
    "lint": "eslint",
    "build": "tsc",
    "overload": "artillery run load.yml --insecure",
    "test:unit": "jest --coverage",
    "test:e2e": "TEST_PACKAGE=e2e jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:e2e:watch": "TEST_PACKAGE=e2e jest --watch",
    "test": "concurrently \"npm:test:unit\" \"npm:test:e2e\""
  },
  "dependencies": {
    "@discordjs/opus": "0.3.2",
    "@google-cloud/pubsub": "2.3.0",
    "@google-cloud/secret-manager": "3.1.0",
    "@google-cloud/speech": "4.1.1",
    "@types/express": "4.17.7",
    "@types/node": "14.0.26",
    "@types/node-telegram-bot-api": "0.50.2",
    "@types/parse": "2.12.2",
    "@types/pg": "7.14.4",
    "@types/winston-loggly-bulk": "3.0.0",
    "aws-sdk": "2.720.0",
    "axios": "0.19.2",
    "express": "4.17.1",
    "ffmpeg-static": "4.2.6",
    "google-gax": "2.6.3",
    "kleur": "4.0.2",
    "nanoid": "3.1.10",
    "ngrok": "3.2.7",
    "node-fetch": "2.6.0",
    "node-telegram-bot-api": "0.50.0",
    "parse": "2.15.0",
    "pg": "8.3.0",
    "prism-media": "1.2.2",
    "signal-exit": "3.0.3",
    "strip-ansi": "6.0.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "waveheader": "0.0.2",
    "winston": "3.3.3",
    "winston-loggly-bulk": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "artillery": "1.6.1",
    "artillery-plugin-expect": "1.3.0",
    "babel-jest": "26.1.0",
    "concurrently": "5.2.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "jest-sonar": "0.2.10",
    "lint-staged": "10.2.11",
    "nock": "13.0.2",
    "prettier": "2.0.5",
    "query-string": "6.13.1",
    "supertest": "4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,html,yml}": [
      "npm run format"
    ],
    "*.{js,ts,json}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "author": "n0th1ng-else",
  "repository": {
    "url": "https://github.com/n0th1ng-else/voice-to-text-bot"
  },
  "bugs": {
    "url": "https://github.com/n0th1ng-else/voice-to-text-bot/issues"
  },
  "license": "MIT",
  "keywords": [
    "telegram",
    "bot",
    "speech-to-text",
    "recognition",
    "express"
  ]
}
