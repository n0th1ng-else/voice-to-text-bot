{
  "scripts": {
    "import": "node src/scripts/runTs.js import ts",
    "chart": "node src/scripts/runTs.js chart ts",
    "start": "node src/scripts/runTs.js start ts",
    "dev": "node src/scripts/runTs.js dev ts",
    "cluster": "node src/scripts/runTs.js cluster ts",
    "start:js": "node dist/src/scripts/runJs.js start js",
    "dev:js": "node dist/src/scripts/runJs.js dev js",
    "cluster:js": "node dist/src/scripts/runJs.js cluster js",
    "format": "prettier --write",
    "lint": "eslint",
    "build": "tsc",
    "overload": "artillery run load.yml --insecure",
    "test:unit": "jest --coverage",
    "test:e2e": "TEST_PACKAGE=e2e jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:e2e:watch": "TEST_PACKAGE=e2e jest --watch",
    "test": "concurrently \"npm:test:unit\" \"npm:test:e2e\""
  },
  "dependencies": {
    "@google-cloud/pubsub": "2.16.1",
    "@google-cloud/secret-manager": "3.9.0",
    "@google-cloud/speech": "4.5.5",
    "aws-sdk": "2.958.0",
    "axios": "0.21.2",
    "colorette": "1.2.2",
    "express": "4.17.1",
    "ffmpeg-static": "4.4.0",
    "google-gax": "2.20.0",
    "nanoid": "3.1.23",
    "ngrok": "4.1.0",
    "node-fetch": "2.6.1",
    "node-telegram-bot-api": "0.54.0",
    "parse": "3.3.0",
    "pg": "8.7.1",
    "prism-media": "1.3.1",
    "signal-exit": "3.0.3",
    "strip-ansi": "6.0.0",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "waveheader": "0.0.2",
    "winston": "3.3.3",
    "winston-loggly-bulk": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@types/express": "4.17.13",
    "@types/node": "14.14.14",
    "@types/node-telegram-bot-api": "0.51.4",
    "@types/parse": "2.18.8",
    "@types/pg": "8.6.1",
    "@types/supertest": "2.0.11",
    "@types/winston-loggly-bulk": "3.0.1",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "artillery": "1.7.6",
    "artillery-plugin-expect": "1.5.0",
    "babel-jest": "27.0.6",
    "concurrently": "6.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.1",
    "jest": "26.6.3",
    "jest-sonar": "0.2.12",
    "lint-staged": "11.1.1",
    "nock": "13.1.1",
    "prettier": "2.3.2",
    "query-string": "7.0.1",
    "supertest": "6.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,html,yml}": [
      "npm run format"
    ],
    "*.{js,ts,json}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "author": "n0th1ng-else",
  "repository": {
    "url": "https://github.com/n0th1ng-else/voice-to-text-bot"
  },
  "bugs": {
    "url": "https://github.com/n0th1ng-else/voice-to-text-bot/issues"
  },
  "license": "MIT",
  "keywords": [
    "telegram",
    "bot",
    "speech-to-text",
    "recognition",
    "express"
  ]
}
