{
  "name": "audio-mess-bot",
  "description": "Telegram Bot Converts Voice Messages Into Text",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.16.0"
  },
  "type": "module",
  "scripts": {
    "import": "ts-node-esm -r ./init.cjs src/scripts/run.js import ts",
    "chart": "ts-node-esm -r ./init.cjs src/scripts/run.js chart ts",
    "start": "ts-node-esm -r ./init.cjs src/scripts/run.js start ts",
    "dev": "ts-node-esm -r ./init.cjs src/scripts/run.js dev ts",
    "cluster": "ts-node-esm -r ./init.cjs src/scripts/run.js cluster ts",
    "start:js": "node -r ./init.cjs --experimental-loader newrelic/esm-loader.mjs dist/src/scripts/run.js start js",
    "dev:js": "node -r ./init.cjs --experimental-loader newrelic/esm-loader.mjs dist/src/scripts/run.js dev js",
    "cluster:js": "node -r ./init.cjs --experimental-loader newrelic/esm-loader.mjs dist/src/scripts/run.js cluster js",
    "format": "prettier --write",
    "lint": "eslint",
    "build": "tsc",
    "test:runner": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:load": "artillery run load.yml --insecure",
    "test:unit": "npm run test:runner --coverage",
    "test:e2e": "TEST_PACKAGE=e2e npm run test:runner --coverage",
    "test:unit:watch": "npm run test:runner --watch",
    "test:e2e:watch": "TEST_PACKAGE=e2e npm run test:runner --watch",
    "test": "concurrently \"npm:test:unit\" \"npm:test:e2e\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@amplitude/node": "1.10.2",
    "@google-cloud/pubsub": "2.16.1",
    "@google-cloud/secret-manager": "3.9.0",
    "@google-cloud/speech": "4.5.5",
    "aws-sdk": "2.1171.0",
    "axios": "1.2.0",
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "ffmpeg-static": "5.1.0",
    "google-gax": "2.20.0",
    "nanoid": "3.3.1",
    "newrelic": "9.8.0",
    "ngrok": "4.3.3",
    "node-telegram-bot-api": "0.59.0",
    "parse": "3.4.4",
    "pg": "8.8.0",
    "picocolors": "^1.0.0",
    "prism-media": "1.3.4",
    "signal-exit": "3.0.7",
    "strip-ansi": "6.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "winston": "3.8.2",
    "winston-loggly-bulk": "3.2.1",
    "winston-logsene": "^2.1.1"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/node": "18.8.2",
    "@types/node-telegram-bot-api": "0.57.1",
    "@types/parse": "2.18.18",
    "@types/pg": "8.6.5",
    "@types/supertest": "2.0.12",
    "@types/winston-loggly-bulk": "3.0.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "artillery": "2.0.0-23",
    "artillery-plugin-expect": "2.1.0",
    "concurrently": "7.4.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "jest": "29.4.1",
    "jest-sonar": "0.2.15",
    "lint-staged": "13.0.3",
    "nock": "13.3.0",
    "prettier": "2.7.1",
    "ts-jest": "29.0.5",
    "query-string": "8.1.0",
    "supertest": "6.3.3"
  },
  "lint-staged": {
    "*.{md,html,yml}": [
      "npm run format"
    ],
    "*.{js,ts,json}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "author": "n0th1ng-else",
  "repository": {
    "url": "https://github.com/n0th1ng-else/voice-to-text-bot"
  },
  "bugs": {
    "url": "https://github.com/n0th1ng-else/voice-to-text-bot/issues"
  },
  "license": "MIT",
  "keywords": [
    "telegram",
    "bot",
    "speech-to-text",
    "recognition",
    "express",
    "speech",
    "voice",
    "text",
    "transcribe",
    "cognitive"
  ]
}
